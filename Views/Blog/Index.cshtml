@model IEnumerable<ProyectoFinal.Models.Blog>
@{
    ViewBag.Title = "Index";
}
@foreach (var item in Model) {
    <div>
    <h3>@Html.DisplayNameFor(model => model.Nombre)</h3>    
            @Html.DisplayFor(modelItem => item.Nombre)

    <h3>@Html.DisplayNameFor(model => model.Informacion)</h3>     
            @Html.DisplayFor(modelItem => item.Informacion)
    </div>
    
}  
